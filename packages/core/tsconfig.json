{
	"compilerOptions": {
		"module": "commonjs",
		"declaration": true,
		"removeComments": true,
		"noLib": false,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"target": "es6",
		"sourceMap": true,
		"lib": ["es6"],
		"preserveSymlinks": true,
		"outDir": "./lib",
		"noImplicitAny": false,
		"esModuleInterop": true
	}
}
